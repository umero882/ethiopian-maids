{
  "name": "ethio-maids",
  "type": "module",
  "version": "0.1.0-alpha.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:helper": "node scripts/dev-server.js",
    "dev:open": "vite --open",
    "api:start": "cd server && npm start",
    "api:dev": "cd server && npm run dev",
    "dev:with-api": "concurrently \"npm run dev\" \"npm run api:start\"",
    "build": "npm run env:validate && vite build",
    "env:validate": "node scripts/validate-env.cjs",
    "security:check": "bash scripts/security-check.sh",
    "cleanup:console": "node scripts/remove-console-logs.cjs",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 600",
    "lint:ci": "eslint . --max-warnings 200",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:jest": "node scripts/test-with-cleanup.cjs",
    "test:jest:direct": "node --max-old-space-size=16384 --expose-gc ./node_modules/jest/bin/jest.js --runInBand --logHeapUsage --detectOpenHandles --forceExit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:stripe": "node src/test/stripeIntegrationTest.js",
    "verify:stripe": "node scripts/verify-stripe-integration.js",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "migrate": "node scripts/run-migrations.js",
    "db:setup": "node database/scripts/setup-database.js",
    "db:test": "node database/scripts/test-database.js",
    "db:diagnostic": "node src/scripts/databaseDiagnostic.js",
    "test:supabase": "node scripts/test-supabase-integration.js",
    "production:check": "node scripts/production-check.js",
    "production:build": "npm run production:check && npm run build",
    "production:deploy": "npm run production:build && npm run preview",
    "test:connection": "node scripts/test-connection.js",
    "test:production": "npm run test:connection && npm run build",
    "schema:check": "node -e \"import('./src/services/schemaMigrationService.js').then(m => m.default.runProductionReadinessCheck().then(console.log))\"",
    "config:validate": "node scripts/test-connection.js",
    "security:audit": "node scripts/security-audit.js",
    "security:audit:supabase": "node scripts/security-audit-supabase.js",
    "security:fix": "psql -f database/migrations/999_comprehensive_security_audit.sql",
    "admin:setup": "node scripts/setup-admin-panel.js",
    "deploy": "node scripts/deploy-vercel.js",
    "deploy:prod": "node scripts/deploy-vercel.js --prod",
    "deploy:setup": "node scripts/deploy-vercel.js --set-env",
    "deploy:verify": "node scripts/verify-deployment.cjs",
    "deploy:check": "node scripts/post-deployment-check.cjs",
    "deploy:health": "node scripts/deployment-health-monitor.cjs",
    "deploy:quick": "node scripts/quick-deploy.cjs",
    "test:migration": "node scripts/test-migration.js",
    "test:phase1": "node scripts/test-phase1-changes.js",
    "test:db": "node scripts/test-database-connection.js",
    "validate:staging": "node scripts/validate-staging.js",
    "test:ci": "vitest run --coverage",
    "test:unit": "vitest run --testPathPattern=__tests__",
    "db:migrate:schema": "psql -f database/migrations/998_schema_consistency_fix.sql",
    "sanitize:repo": "node scripts/cleanup-nonascii.js",
    "analyze:bundle": "npm run build && node scripts/analyze-bundle.js",
    "analyze:deps": "npx depcheck",
    "performance:audit": "npm run build && npx lighthouse http://localhost:4173 --output=json --output-path=./lighthouse-report.json",
    "supabase:login": "supabase login",
    "supabase:link": "supabase link",
    "supabase:secrets": "bash scripts/setup-stripe-secrets.sh",
    "supabase:deploy": "bash scripts/deploy-edge-functions.sh",
    "supabase:logs": "supabase functions logs",
    "stripe:setup": "npm run supabase:secrets && npm run supabase:deploy"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
