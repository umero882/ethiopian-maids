    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.0",
    "open": "^10.2.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "socket.io": "^4.7.4",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.0",
    "vite": "^7.1.5",
