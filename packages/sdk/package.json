{
  "name": "@ethio-maids/sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "Ethiopian Maids SDK - Type-safe API client generated from OpenAPI",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "generate": "openapi-typescript ../../services/api/openapi.yaml -o ./src/schema.d.ts",
    "build": "tsc && copy src\\*.js dist\\ && copy src\\*.d.ts dist\\",
    "test": "vitest"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.0"
  },
  "devDependencies": {
    "openapi-typescript": "^6.7.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
